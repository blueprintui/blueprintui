{
  "name": "vue-example",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "wireit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@blueprintui/components": "workspace:*",
    "@blueprintui/layout": "workspace:*",
    "@blueprintui/themes": "workspace:*",
    "@blueprintui/typography": "workspace:*",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@types/node": "20.14.5",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/tsconfig": "0.7.0",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vue-tsc": "3.0.5"
  },
  "wireit": {
    "build": {
      "command": "vue-tsc --build --force && vite build",
      "dependencies": [
        "../../packages/components:build",
        "../../packages/grid:build",
        "../../packages/icons:build",
        "../../packages/layout:build",
        "../../packages/themes:build",
        "../../packages/typography:build"
      ],
      "files": [
        "src",
        "index.html",
        "tsconfig.json",
        "tsconfig.app.json",
        "vite.config.ts"
      ],
      "output": [
        "dist"
      ]
    },
    "test": {
      "command": "vitest run",
      "dependencies": [
        "build"
      ],
      "files": [
        "src",
        "index.html",
        "tsconfig.json",
        "tsconfig.app.json",
        "vite.config.ts",
        "vitest.config.ts"
      ],
      "output": []
    }
  }
}
