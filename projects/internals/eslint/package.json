    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jasmine": "4.2.2",
    "eslint-plugin-lit": "2.1.1",
    "eslint-plugin-lit-a11y": "5.1.1",
    "eslint-plugin-wc": "3.0.1"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "wireit": {
    "ci": {
      "files": [
