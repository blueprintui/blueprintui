{
  "name": "@blueprintui/grid",
  "version": "2.7.0",
  "type": "module",
  "files": [
    "package.json",
    "CHANGELOG.md",
    "README.md",
    "LICENSE.md",
    "dist/**/*"
  ],
  "customElements": "./dist/custom-elements.json",
  "html.customData": [
    "./vscode.html-custom-data.json"
  ],
  "css.customData": [
    "./vscode.css-custom-data.json"
  ],
  "keywords": [
    "web component",
    "design system",
    "ui"
  ],
  "description": "",
  "homepage": "https://blueprintui.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueprintui/blueprintui.git"
  },
  "bugs": {
    "url": "https://github.com/blueprintui/blueprintui/issues"
  },
  "author": "Crylan Software",
  "license": "MIT",
  "sideEffects": [
    "./dist/include/*.js"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./custom-elements.json": "./dist/custom-elements.json",
    "./include/*.js": {
      "types": "./dist/include/*.d.ts",
      "default": "./dist/include/*.js"
    },
    "./csv/*.js": {
      "types": "./dist/csv/*.d.ts",
      "default": "./dist/csv/*.js"
    },
    "./internals": {
      "types": "./dist/internals/index.d.ts",
      "default": "./dist/internals/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "default": "./dist/*/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@blueprintui/components": "workspace:^",
    "@blueprintui/crane": "workspace:^",
    "@blueprintui/icons": "workspace:^",
    "@blueprintui/orbit": "workspace:^",
    "@blueprintui/themes": "workspace:^",
    "@blueprintui/typewriter": "workspace:^",
    "lit": "catalog:",
    "tslib": "catalog:"
  },
  "devDependencies": {
    "@blueprintui-internals/eslint": "workspace:*",
    "@blueprintui/cli": "catalog:",
    "@blueprintui/drafter": "catalog:",
    "@blueprintui/layout": "workspace:^",
    "@blueprintui/test": "workspace:^",
    "@blueprintui/typography": "workspace:^",
    "@blueprintui/virtual": "workspace:^",
    "@types/jasmine": "catalog:",
    "@web/dev-server": "catalog:",
    "@web/dev-server-esbuild": "catalog:",
    "@web/dev-server-rollup": "catalog:",
    "@web/test-runner": "catalog:",
    "@web/test-runner-core": "catalog:",
    "@web/test-runner-playwright": "catalog:",
    "@web/test-runner-visual-regression": "catalog:",
    "del-cli": "catalog:",
    "jasmine": "catalog:",
    "jasmine-core": "catalog:",
    "modern-normalize": "catalog:",
    "playwright": "catalog:",
    "publint": "catalog:",
    "web-test-runner-jasmine": "catalog:",
    "web-test-runner-performance": "catalog:"
  },
  "scripts": {
    "ci": "wireit",
    "start": "wireit",
    "build": "wireit",
    "build:watch": "wireit",
    "drafter": "wireit",
    "drafter:watch": "wireit",
    "test": "wireit",
    "test:watch": "wireit",
    "test:unit": "wireit",
    "test:coverage": "wireit",
    "test:a11y": "wireit",
    "test:performance": "wireit",
    "test:visual": "wireit",
    "test:visual:update": "wireit",
    "test:api": "wireit",
    "test:api:update": "wireit",
    "sourcemap": "wireit"
  },
  "wireit": {
    "ci": {
      "dependencies": [
        "lint",
        "build",
        "drafter",
        "test"
      ]
    },
    "start": {
      "dependencies": [
        "build:watch",
        "drafter:watch"
      ]
    },
    "build": {
      "command": "bp build",
      "dependencies": [
        "../internals/test:build",
        "../components:build",
        "../crane:build",
        "../icons:build",
        "../themes:build",
        "../typewriter:build",
        "../layout:build",
        "../typography:build",
        "../virtual:build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "!./src/**/*.performance.ts",
        "./blueprint.config.js",
        "./tsconfig.lib.json",
        "./tsconfig.json",
        "./package.json"
      ],
      "output": [
        "./dist"
      ]
    },
    "build:watch": {
      "command": "bp build --watch",
      "service": true,
      "dependencies": [
        "build"
      ]
    },
    "drafter": {
      "command": "drafter build",
      "files": [
        "./src/**/*",
        "./blueprint.config.js"
      ],
      "output": [
        "./.drafter"
      ],
      "dependencies": [
        "build"
      ]
    },
    "drafter:watch": {
      "command": "drafter build --watch",
      "service": true,
      "dependencies": [
        "build"
      ]
    },
    "lint": {
      "dependencies": [
        "lint:eslint",
        "lint:publish"
      ]
    },
    "lint:eslint": {
      "command": "eslint -c ./eslint.config.js --color",
      "dependencies": [
        "../internals/eslint:ci"
      ],
      "files": [
        "./src/**/*.js",
        "./src/**/*.ts",
        ".eslint.config.js",
        ".eslintcache"
      ],
      "output": []
    },
    "lint:publish": {
      "command": "publint",
      "dependencies": [
        "build"
      ],
      "files": [
        "./dist",
        "./package.json",
        "./README.md",
        "./LICENSE.md"
      ],
      "output": []
    },
    "test": {
      "dependencies": [
        "test:api",
        "test:unit",
        "test:visual"
      ]
    },
    "test:watch": {
      "command": "node node_modules/@blueprintui/test/dist/wtr.js --watch",
      "service": true
    },
    "test:unit": {
      "command": "del ./.coverage && node node_modules/@blueprintui/test/dist/wtr.js",
      "dependencies": [
        "../internals/test:build",
        "build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.performance.ts",
        "./blueprint.config.js",
        "./web-dev-server.config.mjs",
        "./web-test-runner.config.mjs",
        "./tsconfig.spec.json",
        "./tsconfig.json"
      ],
      "output": []
    },
    "test:coverage": {
      "command": "del ./.coverage && node node_modules/@blueprintui/test/dist/wtr.js --coverage",
      "dependencies": [
        "../internals/test:build",
        "build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.performance.ts",
        "./blueprint.config.js",
        "./web-dev-server.config.mjs",
        "./web-test-runner.config.mjs",
        "./tsconfig.spec.json",
        "./tsconfig.json"
      ],
      "output": [
        "./.coverage"
      ]
    },
    "test:a11y": {
      "command": "node node_modules/@blueprintui/test/dist/wtr.js --config ./web-test-runner.a11y.mjs",
      "dependencies": [
        "../internals/test:build",
        "build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.performance.ts",
        "./web-dev-server.config.mjs",
        "./web-test-runner.config.mjs"
      ],
      "output": []
    },
    "test:performance": {
      "command": "node node_modules/@blueprintui/test/dist/wtr.js --config ./web-test-runner.performance.mjs",
      "dependencies": [
        "../internals/test:build",
        "build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "./web-dev-server.config.mjs",
        "./web-test-runner.config.mjs",
        "./web-test-runner.performance.mjs"
      ],
      "output": []
    },
    "test:visual": {
      "command": "web-test-runner --config ./web-test-runner.visual.mjs",
      "dependencies": [
        "../internals/test:build",
        "build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "./web-dev-server.config.mjs",
        "./web-test-runner.visual.mjs"
      ],
      "output": []
    },
    "test:visual:update": {
      "command": "node node_modules/@blueprintui/test/dist/wtr.js --update-visual-baseline --config ./web-test-runner.visual.mjs",
      "dependencies": [
        "build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "./web-dev-server.config.mjs",
        "./web-test-runner.visual.mjs"
      ],
      "output": [
        "screenshots/Chromium/baseline/**/*.png"
      ]
    },
    "test:api": {
      "command": "bp api --test",
      "dependencies": [
        "build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "!./src/**/*.performance.ts"
      ],
      "output": []
    },
    "test:api:update": {
      "command": "bp api --update",
      "dependencies": [
        "build"
      ]
    },
    "sourcemap": {
      "command": "source-map-explorer ./.performance/**/*.js",
      "dependencies": [
        "build"
      ]
    }
  }
}
