{
  "name": "@blueprintui/icons",
  "version": "2.1.6",
  "type": "module",
  "customElements": "./dist/custom-elements.json",
  "html.customData": [
    "./vscode.html-custom-data.json"
  ],
  "css.customData": [
    "./vscode.css-custom-data.json"
  ],
  "license": "MIT",
  "author": "Crylan Software",
  "homepage": "https://blueprintui.dev",
  "description": "Web Component Icon Library for BlueprintUI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blueprintui/blueprintui.git"
  },
  "bugs": {
    "url": "https://github.com/blueprintui/blueprintui/issues"
  },
  "keywords": [
    "web components",
    "blueprintui"
  ],
  "files": [
    "package.json",
    "CHANGELOG.md",
    "README.md",
    "LICENSE.md",
    "dist/**/*"
  ],
  "sideEffects": [
    "./dist/include.js",
    "./dist/shapes/*.js",
    "./dist/internals/icon.service.js"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./custom-elements.json": "./dist/custom-elements.json",
    "./shapes/*": {
      "types": "./dist/shapes/*",
      "default": "./dist/shapes/*"
    },
    "./include": {
      "types": "./dist/include.d.ts",
      "default": "./dist/include.js"
    },
    "./include.js": {
      "types": "./dist/include.d.ts",
      "default": "./dist/include.js"
    },
    "./internals": {
      "types": "./dist/internals/index.d.ts",
      "default": "./dist/internals/index.js"
    },
    "./internals/index.js": {
      "types": "./dist/internals/index.d.ts",
      "default": "./dist/internals/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "default": "./dist/*/index.js"
    }
  },
  "dependencies": {
    "lit": "catalog:",
    "tslib": "catalog:"
  },
  "optionalDependencies": {
    "@blueprintui/themes": "workspace:^"
  },
  "devDependencies": {
    "@blueprintui/cli": "catalog:",
    "@blueprintui/drafter": "catalog:",
    "@blueprintui/test": "workspace:^",
    "@blueprintui/themes": "workspace:^",
    "@blueprintui/layout": "workspace:^",
    "@blueprintui/typography": "workspace:^",
    "@types/jasmine": "catalog:",
    "@web/dev-server": "catalog:",
    "@web/dev-server-esbuild": "catalog:",
    "@web/dev-server-rollup": "catalog:",
    "@web/test-runner": "catalog:",
    "@web/test-runner-core": "catalog:",
    "@web/test-runner-playwright": "catalog:",
    "@web/test-runner-visual-regression": "catalog:",
    "jasmine": "catalog:",
    "jasmine-core": "catalog:",
    "typescript": "catalog:",
    "playwright": "catalog:",
    "publint": "catalog:",
    "web-test-runner-jasmine": "catalog:",
    "web-test-runner-performance": "catalog:"
  },
  "scripts": {
    "ci": "wireit",
    "start": "wireit",
    "build": "wireit",
    "build:svg": "wireit",
    "build:watch": "wireit",
    "drafter": "wireit",
    "drafter:watch": "wireit",
    "test": "wireit",
    "test:watch": "wireit",
    "test:unit": "wireit",
    "test:coverage": "wireit",
    "test:a11y": "wireit",
    "test:visual": "wireit",
    "test:visual:update": "wireit",
    "test:api": "wireit",
    "test:api:update": "wireit",
    "test:performance": "wireit",
    "sourcemap": "wireit"
  },
  "wireit": {
    "ci": {
      "dependencies": [
        "lint",
        "build",
        "test",
        "test:performance"
      ]
    },
    "start": {
      "dependencies": [
        "build:watch",
        "drafter:watch"
      ]
    },
    "lint": {
      "dependencies": [
        "lint:publish"
      ]
    },
    "lint:publish": {
      "command": "publint",
      "dependencies": [
        "build"
      ],
      "files": [
        "./dist",
        "./package.json",
        "./README.md",
        "./LICENSE.md"
      ],
      "output": []
    },
    "build": {
      "command": "bp build",
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "!./src/**/*.performance.ts",
        "./blueprint.config.js",
        "./package.json"
      ],
      "output": [
        "./dist"
      ],
      "dependencies": [
        "../themes:build",
        "build:svg"
      ]
    },
    "build:watch": {
      "command": "bp build --watch",
      "service": true,
      "dependencies": [
        "build"
      ]
    },
    "build:svg": {
      "command": "node ./svgs.js",
      "files": [
        "./svgs/*.svg",
        "./svgs.js"
      ],
      "output": [
        "./src/shapes/**/*"
      ]
    },
    "drafter": {
      "command": "drafter build",
      "files": [
        "./src/**/*",
        "./blueprint.config.js",
        "./package.json"
      ],
      "output": [
        "./.drafter"
      ],
      "dependencies": [
        "build",
        "../layout:build"
      ]
    },
    "drafter:watch": {
      "command": "drafter build --watch",
      "service": true,
      "dependencies": [
        "build"
      ]
    },
    "test": {
      "dependencies": [
        "test:api",
        "test:unit",
        "test:visual"
      ]
    },
    "test:watch": {
      "command": "node node_modules/@blueprintui/test/dist/wtr.js --watch",
      "service": true
    },
    "test:unit": {
      "command": "node node_modules/@blueprintui/test/dist/wtr.js",
      "files": [
        "./src/**/*",
        "./package.json",
        "./blueprint.config.js",
        "./web-dev-server.config.mjs",
        "./web-test-runner.config.mjs"
      ],
      "dependencies": [
        "../test:build",
        "build"
      ],
      "output": []
    },
    "test:coverage": {
      "command": "node node_modules/@blueprintui/test/dist/wtr.js --coverage",
      "files": [
        "./src/**/*",
        "./package.json",
        "./blueprint.config.js",
        "./web-dev-server.config.mjs",
        "./web-test-runner.config.mjs"
      ],
      "output": [
        "./.coverage"
      ],
      "dependencies": [
        "../test:build"
      ]
    },
    "test:a11y": {
      "command": "node node_modules/@blueprintui/test/dist/wtr.js --config ./web-test-runner.a11y.mjs",
      "files": [
        "./src/**/*",
        "./package.json",
        "./web-dev-server.config.mjs"
      ],
      "dependencies": [
        "../test:build",
        "build"
      ],
      "output": []
    },
    "test:performance": {
      "command": "node node_modules/@blueprintui/test/dist/wtr.js --config ./web-test-runner.performance.mjs",
      "files": [
        "./src/**/*",
        "./package.json",
        "./web-dev-server.config.mjs",
        "web-test-runner.performance.mjs"
      ],
      "dependencies": [
        "../test:build",
        "build"
      ],
      "output": []
    },
    "test:visual": {
      "command": "web-test-runner --config ./web-test-runner.visual.mjs",
      "dependencies": [
        "build",
        "../layout:build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "./web-dev-server.config.mjs",
        "./web-test-runner.visual.mjs"
      ],
      "output": []
    },
    "test:visual:update": {
      "command": "node node_modules/@blueprintui/test/dist/wtr.js --update-visual-baseline --config ./web-test-runner.visual.mjs",
      "dependencies": [
        "build"
      ],
      "files": [
        "./src/**/*",
        "!./src/**/*.spec.ts",
        "./web-dev-server.config.mjs",
        "./web-test-runner.visual.mjs"
      ],
      "output": [
        "screenshots/Chromium/baseline/**/*.png"
      ]
    },
    "test:api": {
      "command": "bp api --test",
      "dependencies": [
        "build"
      ],
      "files": [
        "./src/**/*",
        "./package.json"
      ],
      "output": []
    },
    "test:api:update": {
      "command": "bp api --update",
      "dependencies": [
        "build"
      ]
    },
    "sourcemap": {
      "command": "pnpm dlx source-map-explorer ./**/*.js",
      "dependencies": [
        "build"
      ]
    }
  }
}
