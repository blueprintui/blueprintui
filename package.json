{
  "name": "blueprint-css",
  "version": "2.2.2",
  "description": "Blueprint CSS, Responsive CSS Flexbox Grid",
  "main": "gulpfile.js",
  "scripts": {
    "start": "node ./node_modules/gulp/bin/gulp.js -- watch",
    "lint": "sass-lint -v -q",
    "build": "node ./node_modules/gulp/bin/gulp.js -- build",
    "build--prod": "npm run build",
    "test": "cd ./node_modules/backstopjs && npm run test",
    "baseline": "cd ./node_modules/backstopjs && npm run reference",
    "deploy": "npm run build--prod && firebase deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coryrylan/blueprint-css.git"
  },
  "author": "Cory Rylan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coryrylan/blueprint-css/issues"
  },
  "homepage": "https://blueprintcss.io",
  "devDependencies": {
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-help": "^1.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "browser-sync": "^2.18.8",
    "backstopjs": "^2.5.1",
    "sass-lint": "^1.10.2"
  }
}
